# seaborn notebook context
# see seaborn.rcmod.plotting_context()
font.size: 12
axes.labelsize: 12
axes.titlesize: 12
xtick.labelsize: 11
ytick.labelsize: 11
legend.fontsize: 11

axes.linewidth: 1.25
grid.linewidth: 1
